<h2>Latest Blog Articles</h2>
<ul>
<% @articles.select(&:link).each do |item| %>
    <li>
        <a href='<%= item.link.href.strip %>' target='_blank'><%= item.title.content %></a>
    </li>
<% end %>
</ul>

<h2>Products</h2>
<ul>
<% @products.each do |product| %>
    <li>
        <a href='<%= product.url %>'><%= product.title %></a>
    </li>
<% end %>
</ul>

